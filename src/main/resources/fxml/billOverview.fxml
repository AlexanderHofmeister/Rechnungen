<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/style.css"
            xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="de.alexanderhofmeister.rechnungen.view.BillOverviewController">
    <Pane minHeight="-Infinity" minWidth="-Infinity" prefHeight="100" prefWidth="1350.0" styleClass="subheading">
        <Label layoutX="15.0" layoutY="24.0" prefHeight="43.0" prefWidth="500.0" text="Rechnungen" textFill="#7c8184">
            <font>
                <Font size="36.0"/>
            </font>
        </Label>
    </Pane>
    <TextField fx:id="filter" layoutX="14.0" layoutY="113.0" promptText="Suche"/>
    <Button fx:id="newEntity" layoutX="180.0" layoutY="113.0" mnemonicParsing="false" text="Neue Rechnung"/>
    <Label fx:id="hitCount" layoutX="14.0" layoutY="169.0" prefHeight="32.0" prefWidth="216.0"
           stylesheets="@../css/style.css" text="Label">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
    <TableView fx:id="entityTable" layoutX="14.0" layoutY="209.0" prefHeight="504.0" prefWidth="700.0"/>
    <HBox fx:id="pageContainer" layoutX="14.0" layoutY="713.0" prefHeight="43.0" prefWidth="650.0"/>

</AnchorPane>
